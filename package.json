{
  "name": "yarn-upgrade-on-ci",
  "version": "0.0.2",
  "main": "index.js",
  "author": "htwroclau <thajime207@gmail.com>",
  "license": "MIT",
  "bin": "bin/main.js",
  "scripts": {
    "compile": "cross-env NODE_ENV=production babel src --out-dir lib --source-maps inline",
    "lint": "eslint src/ test/",
    "test": "cross-env NODE_ENV=development ava",
    "build": "npm run lint && npm run compile && npm run test",
    "watch": "babel src --out-dir lib --watch --source-maps inline",
    "watch:test": "cross-env NODE_ENV=development ava --watch"
  },
  "dependencies": {
    "@octokit/rest": "^15.8.1",
    "cli-table2": "^0.2.0",
    "colors": "^1.3.0",
    "commander": "^2.15.1",
    "cross-spawn": "^6.0.5",
    "git-url-parse": "^9.0.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mz": "^2.7.0",
    "read-package-json": "^2.0.13",
    "read-package-tree": "^5.2.1",
    "sha.js": "^2.4.11"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chokidar": "^2.0.3",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "{src, test}/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
